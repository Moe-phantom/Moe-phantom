        "thresh = np.percentile(train_proba, 90)\n",
        "train_preds = (train_proba >= thresh).astype(int)\n",
        "\n",
        "# 3. Calculate Key Metrics\n",
        "acc = accuracy_score(y_train, train_preds)\n",
        "f1 = f1_score(y_train, train_preds)\n",
