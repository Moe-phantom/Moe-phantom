      "cell_type": "code",
      "source": [
        "import lightgbm as lgb\n",
        "\n",
        "# ============================================================================\n",
        "# STEP 7.5: TRAIN PHYSICS-ONLY LIGHTGBM (NO DATES)\n",
        "# ============================================================================\n",
        "print(\"\\n\" + \"=\" * 80)\n",
        "print(\"TRAINING PHYSICS-ONLY LIGHTGBM (NO DATES)\")\n",
        "print(\"=\" * 80)\n",
        "\n",
        "# 1. IDENTIFY & DROP ALL DATE FEATURES\n",
        "# We look for 'day', 'month', 'week', 'sin', 'cos' to kill the calendar completely\n",
        "date_keywords = ['day', 'month', 'week', 'year', 'sin', 'cos']\n",
        "cols_to_drop = [c for c in X_train.columns if any(k in c for k in date_keywords)]\n",
        "\n",
        "print(f\"🚫 Dropping {len(cols_to_drop)} calendar features from LGBM:\")\n",
        "print(f\"   {cols_to_drop}\")\n",
        "\n",
        "# Create specific datasets for LGBM\n",
        "X_train_lgbm = X_train.drop(columns=cols_to_drop)\n",
        "X_test_lgbm = X_test.drop(columns=cols_to_drop)\n",
        "\n",
        "# 2. TRAIN LIGHTGBM\n",
        "# We use 'dart' boosting (Dropout) which is great for preventing overfitting\n",
        "lgbm_model = lgb.LGBMClassifier(\n",
        "    n_estimators=1000,\n",
        "    learning_rate=0.01,\n",
        "    max_depth=4,              # Keep it shallow to force generalization\n",
        "    num_leaves=16,            # Limit complexity\n",
        "    subsample=0.6,            # Train on random 60% of data each tree\n",
        "    colsample_bytree=0.6,     # Use random 60% of features each tree\n",
        "    scale_pos_weight=scale_pos_weight, # Use the same weight as XGB\n",
        "    random_state=42,\n",
        "    n_jobs=-1,\n",
        "    importance_type='gain'\n",
        ")\n",
        "\n",
        "print(\"Training LightGBM on physics only...\")\n",
        "lgbm_model.fit(X_train_lgbm, y_train)\n",
        "print(\"✓ LightGBM Trained!\")\n",
        "\n",
        "# 3. GENERATE PREDICTIONS\n",
        "# Note: We use the same threshold strategy we found earlier\n",
        "lgbm_train_proba = lgbm_model.predict_proba(X_train_lgbm)[:, 1]\n",
        "lgbm_test_proba = lgbm_model.predict_proba(X_test_lgbm)[:, 1]\n",
        "\n",
        "# Check Feature Importance to verify 'day_sin' is gone\n",
        "print(\"\\nTop 5 LightGBM Features (Should be Soil/Temp now):\")\n",
        "lgb.plot_importance(lgbm_model, max_num_features=5, importance_type='gain', title='LGBM Physics Features')\n",
        "plt.show()\n",
        "\n",
        "# ============================================================================\n",
        "# STEP 8: ENSEMBLE (AVERAGE THE MODELS)\n",
        "# ============================================================================\n",
        "print(\"\\nCreating Ensemble...\")\n",
        "\n",
        "# Weighted Average: 50% XGB (your rank 2 model) + 50% LGBM (new physics model)\n",
        "# You can tweak this split (e.g., 0.6 / 0.4) based on which model feels safer\n",
        "ensemble_train_proba = (train_proba * 0.5) + (lgbm_train_proba * 0.5)\n",
        "ensemble_test_proba = (test_proba * 0.5) + (lgbm_test_proba * 0.5)\n",
        "\n",
        "# Find best threshold on the ENSEMBLE probabilities\n",
        "best_f1 = 0\n",
        "best_thresh = 0.5\n",
        "for t in np.arange(0.2, 0.9, 0.01):\n",
        "    p = (ensemble_train_proba >= t).astype(int)\n",
        "    s = f1_score(y_train, p)\n",
        "    if s > best_f1:\n",
        "        best_f1 = s\n",
        "        best_thresh = t\n",
        "\n",
        "print(f\"🏆 Best Ensemble F1: {best_f1:.4f} at Threshold {best_thresh:.4f}\")\n",
        "\n",
        "# Apply to Test\n",
        "final_test_preds = (ensemble_test_proba >= best_thresh).astype(int)"
      ],
